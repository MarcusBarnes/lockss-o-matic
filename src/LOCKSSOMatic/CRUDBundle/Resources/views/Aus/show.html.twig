{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Aus</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Plnsid</th>
                <td>{{ entity.plnsId }}</td>
            </tr>
            <tr>
                <th>Auid</th>
                <td>{{ entity.auid }}</td>
            </tr>
            <tr>
                <th>Manifesturl</th>
                <td>{{ entity.manifestUrl }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('aus') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('aus_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
